{
    "name": "nandosr98/laravel-support-center",
    "description": "This is my package laravel-support-center",
    "keywords": [
        "LaravelSupportCenter",
        "laravel",
        "laravel-support-center"
    ],
    "homepage": "https://github.com/nandosr98/laravel-support-center",
    "license": "MIT",
    "authors": [
        {
            "name": "Fernando Sanchez",
            "email": "fsanchezrequ@protonmail.com",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.2",
        "spatie/laravel-package-tools": "^1.16",
        "illuminate/contracts": "^11.0||^12.0",
        "spatie/laravel-medialibrary": "^11.17"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^8.8",
        "orchestra/testbench": "^10.0.0||^9.0.0",
        "pestphp/pest": "^4.0",
        "pestphp/pest-plugin-arch": "^4.0",
        "pestphp/pest-plugin-laravel": "^4.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelSupportCenter\\": "src/",
            "LaravelSupportCenter\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelSupportCenter\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelSupportCenter\\LaravelSupportCenter\\LaravelSupportCenterServiceProvider"
            ],
            "aliases": {
                "LaravelSupportCenter": "LaravelSupportCenter\\LaravelSupportCenter\\Facades\\LaravelSupportCenter"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
